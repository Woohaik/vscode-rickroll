{
	"name": "vscode-rickroll",
	"displayName": "vscode-rickroll",
	"description": "",
	"version": "1.0.0",
	"author": "Woohaik",
	"publisher": "Woohaik",
	"icon": "resources/logo.jpg",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Woohaik/vscode-rickroll"
	},
	"bugs": {
		"url": "https://github.com/Woohaik/vscode-rickroll/issues"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-rickroll.info",
				"title": "RickRoll info"
			},
			{
				"command": "vscode-rickroll.removeBG",
				"title": "RickRoll remove Rick"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/play-sound": "^1.1.0",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
